{
  "name": "parcelv2-path-problem",
  "version": "1.0.0",
  "scripts": {
    "clean": "npm-run-all --parallel clean:*",
    "clean:cache": "npx shx rm -rf .parcel-cache",
    "clean:dist": "npx shx rm -rf output",
    "dev": "npm-run-all clean --parallel dev:*",
    "dev:parcel": "parcel --dist-dir output",
    "build": "npm-run-all clean build:*",
    "build:parcel": "parcel build --dist-dir output"
  },
  "author": "Kirill Ivanov",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0"
  },
  "source": ["src/index.html", "src/about/index.html"]
}
